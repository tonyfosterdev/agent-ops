{
  "name": "agentops",
  "version": "1.0.0",
  "description": "Durable ops agent framework with Inngest AgentKit",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "dev:server": "tsx watch src/server.ts",
    "start": "node --import ./dist/telemetry.js ./dist/server.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@inngest/agent-kit": "^0.13.2",
    "@opentelemetry/auto-instrumentations-node": "^0.52.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.56.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "hono": "^4.6.12",
    "inngest": "^3.45.0",
    "postgres": "^3.4.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
